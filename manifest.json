{
    "name": "Ticket printer",
    "short_name": "TicketPrinter",
    "description": "Application to print tickets",
    "scope": "./",
    "start_url": "./",
    "display": "standalone",
    "background_color": "#ffffff",
    "theme_color": "#000000",
    "prefer_related_applications": true,
    "share_target": {
      "action": "/impresionTicket/",
      "method": "POST",
      "enctype": "multipart/form-data",
